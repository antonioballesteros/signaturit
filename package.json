{
  "name": "signaturit",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm run sass\" \"remix dev\"",
    "start": "remix-serve build",
    "sass": "sass --watch app/:app/",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "setup": "prisma migrate dev"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.40",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "sass": "^1.52.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
